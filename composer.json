{
    "name": "devbdaily/laravel-md",
    "description": "Customizable markdown components rendered server-side in Laravel",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Braden Carlson",
            "email": "devbdaily@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "illuminate/support": "^8.2",
        "league/commonmark": "^1.3"
    },
    "autoload": {
        "psr-4": {
            "DevBDaily\\LaravelMD\\": [
                "src/"
            ]
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^9",
        "mockery/mockery": "^1.4",
        "orchestra/testbench": "^6.0"
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\DevBDaily\\LaravelMD\\": [
                "tests/"
            ]
        }
    }
}
